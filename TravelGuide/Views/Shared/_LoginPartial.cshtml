
@inject SignInManager<AppUser> _signInManager
@inject UserManager<AppUser> _userManager

    <ul class="navbar-nav">
    @if (_signInManager.IsSignedIn(User))
    {

        <li class="nav-item ml-2">
            <a asp-action ="Logout" asp-controller="Account" class="btn btn-danger">Logout</a>
        </li>
    }else
    {
        <li class="nav-item ">
            <a asp-action="Login" asp-controller="Account" class="btn btn-primary ">Login</a>
        </li>
        <li class="nav-item">
            <a asp-action="Register" asp-controller="Account" class="btn btn-outline-primary mx-1">Sign up</a>
        </li>
    }
</ul>